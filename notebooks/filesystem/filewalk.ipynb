    "\n",
    "folder = os.getcwd()\n",
    "\n",
    "tuple_files = os.walk(folder)\n",
    "\n",
    "# [0]: current_directory\n",
    "# [1]: folder in current_directory\n",
    "# [2]: files in current_directory\n",
    "for item in tuple_files:\n",
    "\n",
    "    for file in item[2]:\n",
    "\n",
    "        print(os.path.join(item[0], file))"
